<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog modal-lg"> 
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" id="btn_click" data-dismiss="modal">&times;</button>
          <h4 class="modal-title"> <b>Series 2.1 a</b></h4>
        </div>
        <div class="modal-body" id="modal-id">
        	<div class="row">
        		<div class="col-sm-6 " style="float:right">
        			<div class="col-sm-6 ">
        				<label style="float:right"><b>Series Date</b></label>
        			</div>
    				<div class="col-sm-6 ">
    					<input class="form-control" type="text"  id="modeldate" style="background-color:white" readonly/>
    				</div>   		
        		</div>
        	</div>
        	<div class="row">
        		<div class="col-sm-6 ">
        			<label><b>Host name</b></label>
        			<input type="text" class="form-control" id="hostName">
        		</div>
        		<div class="col-sm-6 ">
    				<label><b>IP address</b></label>
    				<input type="text" class="form-control" id="ipAddress">   		
        		</div>
        	</div>
        	<div class="row">
        		<div class="col-sm-6 ">
        			<label><b>DataCenter</b></label>
        			<input type="text" class="form-control" id="dataCenter">	
        		</div>
        		<div class="col-sm-6 ">
        			<label><b>Application name</b></label>
        			<input type="text" class="form-control" id="applicationName">
        		</div>
        	</div>
        	<div class="row">
        		<div class="col-sm-6 ">
        			<label><b>Display name</b></label>
        			<input type="text" class="form-control" id="displayName">
        		</div>
        		<div class="col-sm-6 ">
        			<label><b>Process name</b></label>
        			<input type="text" class="form-control" id="processName">
        		</div>
        	</div>
        	<div class="row">
        		<div class="col-sm-6 ">
        			<label><b>Process Availability(%)</b></label>
        			<input type="text" class="form-control" id="processAvailability">	
        		</div>
        		<div class="col-sm-6 ">
        			<label><b>Overall Availability(%)</b></label>
        			<input type="text" class="form-control" id="overallAvailability">
        		</div>
        	</div>
        </div>
        <div class="modal-footer">
          <button type="reset" class="btn btn-default" id="clr" onclick="clear()">CLEAR</button>
          <button type="button" class="btn btn-success" id="save" disabled>SAVE</button>
        </div>
      </div>
      
    </div>
</div> 		
		
		<script>
		
		
		
		
		var series_no="tbl_2_1_a",hostName,ipAddress,dataCenter,applicationName,displayName,processName,processAvailability,overallAvailability,series_date;
		var from_date="",to_date="";
		
		
		
		$("#clr").click(function(){
			clear();			 
		});
		$(document).ready(function (){
			$('#modal-id').keyup(function (){
				  hostName = $('#hostName').val();
				  ipAddress = $('#ipAddress').val();
				  dataCenter = $('#dataCenter').val();
				  applicationName = $('#applicationName').val();
				  displayName = $('#displayName').val();
				  processName = $('#processName').val();
				  processAvailability = $('#processAvailability').val();
				  overallAvailability = $('#overallAvailability').val();
				  series_date = $('#modeldate').val();
				
				if(hostName!=''&&ipAddress!=''&&dataCenter!=''&&applicationName!=''&&displayName!=''&&processName!=''&&processAvailability!=''&&overallAvailability!=''){
					document.getElementById("save").disabled = false;
				}
				else{
					document.getElementById("save").disabled = true;
				}
			}) ;
		 });

		function clear(){
			$('#hostName').val('');
			$('#ipAddress').val('');
			$('#dataCenter').val('');
			$('#applicationName').val('');
			$('#displayName').val('');
			$('#processName').val('');
			$('#processAvailability').val('');
			$('#overallAvailability').val('');
		}
		 $("#save").click(function(){ 
			 clear();
			 var x = document.getElementById("myModal");
			 $.ajax({
					type : "post",
					url : path + "/Series_save",
					async : false,
					data:{
						startDate:from_date,
						endDate:to_date,
			 			table_name:series_no,
			 			hostName : hostName,
			 			ipAddress : ipAddress,
			 			dataCenter : dataCenter,
			 			applicationName :applicationName,
			 			displayName : displayName,
			 			processName : processName,
			 			processAvailability : processAvailability,
			 			overallAvailability : overallAvailability,
			 			series_date :series_date
					},
					
					success : function(data) {	
						var status = data.trim();
						if(status=="SUCCESS"){
							$("#btn_click").click();
							location.reload(true);
						}						
					},
					error : function(xhr, ajaxOptions, thrownError) {
						bootbox.alert("err"+xhr);
					},
					complete : function() {
							
					}
				});
			return false;
		 });
		
		
		$(document).ready(function () {
			var currentDate = new Date();
			
			$('#modeldate').datepicker({
    			format : 'mm/dd/yy',
    			changeMonth : true,
    			changeYear : true,
    			changeDate : true,
    			autoclose: true ,
    			endDate: "today"
            }); 
			
			
            $('#fpick').datepicker({
    			format : 'mm/dd/yy',
    			changeMonth : true,
    			changeYear : true,
    			changeDate : true,
    			autoclose: true ,
    			endDate: "today"
            });
            $('#epick').datepicker({                  
				format : 'mm/dd/yy',
				changeMonth : true,
				changeYear : true,
				changeDate : true,
				autoclose: true,
				endDate: "today"
            });
            
            $('#modeldate').datepicker().datepicker('setDate', '-1d');
            $('#fpick').datepicker().datepicker('setDate', '-1d');
            $('#epick').datepicker().datepicker('setDate', '-1d');

            ajaxRetrieveCall();	
        });
		
		 $("#fpick").change(function(){
		    	from_date = $("#fpick").val(); 
		 });
         $("#epick").change(function(){
         		to_date = $("#epick").val(); 
         });
         
        function ajaxRetrieveCall(){
        		
        	$.ajax({
				type : "post",
				url : path + "/Series_retrieve",
				async : false,
				data:{
					table_name:series_no,
					startDate:from_date,
					endDate:to_date
				},
				
				success : function(data) {
					var obj = JSON.parse(data);
					series_table(obj);					
				},
				error : function(xhr, ajaxOptions, thrownError) {
					bootbox.alert("err"+xhr);
				},
				complete : function() {
						
				}
			});
		return false;
        }
				
		
	function getsearch(val){
	
		if(val=="search"){
			from_date = $("#fpick").val(); 
			to_date = $("#epick").val();
			ajaxRetrieveCall(from_date,to_date);
		}
		else if(val=="refresh"){
			location.reload(true);
		}

	
	}


		</script>


